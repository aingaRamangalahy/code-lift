{
  "compilerOptions": {
      "experimentalDecorators": true,
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es5",
      "outDir": "./build",
      "lib": ["es2015"],
      "skipLibCheck": true,
      "baseUrl": "src",
      "paths": {
        "@core/*": ["./core/*"],
        "@config/*": ["./config/*"],
        "@api/*": ["./api/*"],
      }
    },
}